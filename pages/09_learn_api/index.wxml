<!--pages/09_learn_api/index.wxml-->
<view class="house-list">
    <block wx:for="{{houselist}}" wx:key="index">
        <view class="item">
            <image src="{{item.data.image.url}}" mode="heightFix"/>
            <view class="title">{{item.data.houseName}}</view>
        </view>
    </block>
</view>
